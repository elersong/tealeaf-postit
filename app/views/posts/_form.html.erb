<%= render 'shared/errors', :object => @post %>

<%= form_for @post do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  
  <%= f.label :url %>
  <%= f.text_field :url %>
  <br>
  
  <%= f.label :description %>
  <%= f.text_area :description %>
  <br>
  
  <%= f.submit @btn_text, class: "btn btn-primary" %>
  
<% end %>